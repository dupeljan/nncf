{
    "model": "MobileNetV2",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 2,
    "num_classes": 1001,
    "dataset_preprocessing_preset": "imagenet2012_slim",

    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [5, 10],
            "values": [1e-3, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfrecords"

}
